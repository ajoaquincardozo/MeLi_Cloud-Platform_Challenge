{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDB": {
    "ConnectionString": "${MONGODB_CONNECTION_STRING}",
    "DatabaseName": "${MONGODB_URL_DATABASE}",
    "UrlsCollectionName": "${MONGODB_URLS_COLLECTION_NAME}",
    "UrlAnalyticsCollectionName": "${MONGODB_URL_ANALYTICS_COLLECTION_NAME}"
  },
  "GeneralConfig": {
    "BaseUrl": "${GENERAL_CONFIG_BASE_URL}"
  },
  "Redis": {
    "ConnectionString": "${REDIS_CONNECTION_STRING}"
  }
}